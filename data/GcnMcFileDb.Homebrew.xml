<?xml version="1.0" encoding="UTF-8"?>
<GcnMcFileDb>
	<dbInfo>
		<dbFormatVersion>0.2</dbFormatVersion>
		<dbName>GCN Memory Card File Database (Homebrew)</dbName>
		<dbDesc>Definitions for homebrew GameCube games.</dbDesc>
		<version>0.2.1+</version>
		<author>David Korth</author>
		<email>gerbilsoft@gerbilsoft.com</email>
	</dbInfo>
	<file>
		<gameName>NeoPop</gameName>
		<fileInfo>Save File</fileInfo>
		<id6>NEOP00</id6>
		<search>
			<address>0x0800</address>
			<gameDesc>^NeoPop </gameDesc>
			<fileDesc>^(.*)$</fileDesc>
		</search>
		<dirEntry>
			<filename>${F1}</filename>
			<bannerFormat>0x00</bannerFormat>
			<iconAddress>0x0000</iconAddress>
			<iconFormat>0x0002</iconFormat>
			<iconSpeed>0x0001</iconSpeed>
			<permission>0x04</permission>
			<length>2</length>
		</dirEntry>
	</file>
</GcnMcFileDb>
